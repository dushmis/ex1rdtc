{
  "manifest_version": 2,

  "name": "reddit feed reader",
  "description": "reddit feed reader",
  "version": "1.0",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "permissions": [
    "http://www.reddit.com/"
  ],
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "reddit feed reader",
    "default_popup": "news.html"
  },
  "background": {
    "page": "background.html"
    // "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://www.reddit.com/*"],
      "js": ["contentscript.js","app_base.js","angular.min.js","app.js"]
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'"
  // "chrome_url_overrides" : {
  //   "newtab": "news.html"
  // }
}
